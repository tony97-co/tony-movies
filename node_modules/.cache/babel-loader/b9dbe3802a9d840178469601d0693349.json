\n          currentPage += 1;\r\n          this.fetchActors((currentPage += 1));\r\n        }\r\n      };\r\n    },\r\n    next(){\r\n      currentPage += 1;\r\n      this.fetchActors(currentPage);\r\n    },\r\n    previous(){\r\n       currentPage -= 1;\r\n      this.fetchActors(currentPage);\r\n    }\r\n }\r\n\r\n\r\n\r\n\r\n\r\n \r\n\r\n\r\n  }\r\n </script>\r\n\r\n<style>\r\n\r\n</style>"],"sourceRoot":"src/components/actors"}]}